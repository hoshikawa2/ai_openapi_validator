{
  "openapi": "3.0.0",
  "servers": [
    { "url": "/Caminho-backend/api/fees/v2" }
  ],
  "tags": [
    { "name": "Investment Funds" },
    { "name": "Grouping" }
  ],
  "paths": {
    "/investment-funds": {
      "get": {
        "parameters": [
          { "name": "managerDocumentNumber" },
          { "name": "investimentFundDocumentNumber" },
          { "name": "investiment-FundName" },
          { "name": "groupingFundDateTime" },
          { "name": "groupingStatusCode" },
          { "name": "investmentFundClassification" }
        ],
        "responses": {
          "200": { "description": "Ok" },
          "400": { "description": "Bad Request" },
          "401": { "description": "Unauthorized" }
        }
      }
    },
    "/grouping/group": {
      "post": {
        "requestBody": {},
        "responses": { "201": { "description": "Created" } }
      }
    },
    "/grouping/ungroup": {
      "put": {
        "requestBody": {},
        "responses": { "201": { "description": "Sucesso" } }
      }
    }
  },
  "components": {
    "schemas": {
      "InvestmentFund": {
        "properties": {
          "investmentFundClassification": { "type": "string" },
          "investimentFundDocumentNumber": { "type": "string" },
          "investimentFundName": { "type": "string" },
          "groupingCode": { "type": "integer" },
          "groupingStatusCode": { "type": "integer" },
          "userLogin": { "type": "string" },
          "lastUpdateDateTime": { "type": "string" }
        }
      },
      "Error": {
        "properties": {
          "code": { "type": "string" },
          "title": { "type": "string" },
          "detail": { "type": "string" }
        }
      }
    }
  }
}
